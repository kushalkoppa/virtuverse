{
  "name": "virtuverse",
  "version": "1.0.0",
  "description": "VirtuVerse - Complete E2E Virtualization & Simulation Universe with authentication",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "init-admin": "node backend/scripts/initAdmin.js",
    "test": "jest"
  },
  "keywords": [
    "virtuverse",
    "virtuspace",
    "authentication",
    "simulation"
  ],
  "author": "Bosch",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "better-sqlite3": "^9.2.2",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  }
}
