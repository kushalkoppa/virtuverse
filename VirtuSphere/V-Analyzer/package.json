{
  "name": "v-analyzer",
  "version": "1.0.0",
  "description": "V-Analyzer - Analytics and visualization platform in VirtuSphere",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "analytics",
    "visualization",
    "grafana",
    "virtuverse"
  ],
  "author": "Bosch",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1"
  }
}
