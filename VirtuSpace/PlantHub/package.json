{
  "name": "planthub",
  "version": "1.0.0",
  "description": "PlantHub - Unified platform for plant simulation and manufacturing tools in VirtuSpace",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "test": "jest"
  },
  "keywords": [
    "plant-simulation",
    "manufacturing",
    "virtuspace",
    "model-library"
  ],
  "author": "Bosch",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  }
}
