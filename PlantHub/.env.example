# PlantHub Environment Configuration

# Server Configuration
PORT=3002
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Database Configuration (future use)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=planthub
# DB_USER=planthub_user
# DB_PASSWORD=secure_password

# Authentication (future use)
# JWT_SECRET=your_jwt_secret_key
# JWT_EXPIRY=24h

# Tool Integration Endpoints
SIMULINK_API_URL=http://localhost:8081
MODELICA_API_URL=http://localhost:8083
AMESIM_API_URL=http://localhost:8084

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=50MB

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/planthub.log
